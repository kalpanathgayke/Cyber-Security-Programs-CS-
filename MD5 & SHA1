import hashlib
str1 = input("Enter String to Encode: ")
# then sending to md5()
result1 = hashlib.md5(str1.encode())
# printing the equivalent hexadecimal value.
print("The hexadecimal equivalent of md5 is : ", end ="")
print(result1.hexdigest())
result2 = hashlib.sha1(str1.encode())
# printing the equivalent hexadecimal value.
print("The hexadecimal equivalent of SHA1 is : ",end="")
print(result2.hexdigest())
